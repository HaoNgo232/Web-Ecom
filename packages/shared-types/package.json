            },
            "require": {
                "types": "./dist/cjs/payment/enums.d.cts",
                "default": "./dist/cjs/payment/enums.cjs"
            }
        },
        "./products": {
            "import": {
                "types": "./dist/esm/products/index.d.ts",
                "default": "./dist/esm/products/index.js"
            },
