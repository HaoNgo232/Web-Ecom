                "types": "./dist/esm/auth/index.d.ts",
                "default": "./dist/esm/auth/index.js"
            },
            "require": {
                "types": "./dist/cjs/auth/index.d.cts",
                "default": "./dist/cjs/auth/index.cjs"
            }
        },
        "./auth/interfaces": {
            "import": {
                "types": "./dist/esm/auth/interfaces.d.ts"
            },
            "require": {
                "types": "./dist/cjs/auth/interfaces.d.cts"
            }
        },
        "./auth/enums": {
            "import": {
                "types": "./dist/esm/auth/enums.d.ts",
                "default": "./dist/esm/auth/enums.js"
            },
            "require": {
                "types": "./dist/cjs/auth/enums.d.cts",
                "default": "./dist/cjs/auth/enums.cjs"
            }
        },
        "./cart": {
            "import": {
                "types": "./dist/esm/cart/index.d.ts",
                "default": "./dist/esm/cart/index.js"
            },
            "require": {
                "types": "./dist/cjs/cart/index.d.cts",
                "default": "./dist/cjs/cart/index.cjs"
            }
        },
        "./cart/interfaces": {
            "import": {
                "types": "./dist/esm/cart/interfaces.d.ts"
            },
            "require": {
                "types": "./dist/cjs/cart/interfaces.d.cts"
            }
        },
        "./orders": {
            "import": {
                "types": "./dist/esm/orders/index.d.ts",
