            }
        },
        "./orders/enums": {
            "import": {
                "types": "./dist/esm/orders/enums.d.ts",
                "default": "./dist/esm/orders/enums.js"
            },
            "require": {
                "types": "./dist/cjs/orders/enums.d.cts",
                "default": "./dist/cjs/orders/enums.cjs"
            }
        },
        "./payment": {
            "import": {
                "types": "./dist/esm/payment/index.d.ts",
                "default": "./dist/esm/payment/index.js"
            },
            "require": {
                "types": "./dist/cjs/payment/index.d.cts",
                "default": "./dist/cjs/payment/index.cjs"
            }
        },
        "./payment/interfaces": {
